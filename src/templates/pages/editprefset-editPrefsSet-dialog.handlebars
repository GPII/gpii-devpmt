<div class="reveal" id="pmt-dialog-container" data-reveal>
    <button class="close-button" data-close aria-label="Close modal" type="button">
        <span aria-hidden="true">&times;</span>
    </button>

    <h2>{{{messageHelper "gpii_pmt_editPrefsSetDialog_header"}}}</h2>
    {{{messageHelper "gpii_pmt_editPrefsSetDialog_inst"}}}
    <form>
    <label>{{{messageHelper "gpii_pmt_editPrefsSetDialog_idLabel"}}}
        <input id="pmt-edit-prefsSet-id-input" type="text" placeholder="preferences01" />
    </label>
    {{#if prefsSetIdErrors}}
    <div role="alert" class="alert callout" style="display: block;">
        <p><i class="fi-alert"></i>{{prefsSetIdErrors}}</p>
    </div>
    {{/if}}

    <label>{{{messageHelper "gpii_pmt_editPrefsSetDialog_nameLabel"}}}
        <input id="pmt-edit-prefsSet-name-input" type="text" placeholder="preferences01" />
    </label>
    {{#if prefsSetNameErrors}}
    <div role="alert" class="alert callout" style="display: block;">
        <p><i class="fi-alert"></i>{{prefsSetNameErrors}}</p>
    </div>
    {{/if}}


    <div class="button-group">
        <button type="submit" class="success button pmt-confirm-dialog-button">{{{messageHelper "gpii_pmt_saveButton"}}}</button>
        <button type="button"  class="delete button pmt-cancel-dialog-button">{{{messageHelper "gpii_pmt_cancelButton"}}}</button>
    </div>
    </form>
</div>
